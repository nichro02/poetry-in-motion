<h2 id="profileName"><%= currentUser.name %>'s Profile</h2>
<h5 id="joinDate">Posting since <%= moment(currentUser.createdAt).format('MMMM Do, YYYY') %></h5>

<form method="GET" action="/posts/new">
    <input class="newButton" type="submit" value="Create a New Post">
</form>

<h3 class="post">Post History</h3>
<% posts.forEach(post => { %>
    <p class="post"><a href="posts/<%= post.dataValues.id %>"><%= post.dataValues.title %></a></p>
    <p class="postDate">Posted on <%= moment(post.dataValues.createdAt).format('MMMM Do, YYYY') %></p>
<% }) %>

<a class="navArrow" href="/">&larr; Back to Home</a>